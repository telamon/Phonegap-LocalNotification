<?xml version="1.0" encoding="UTF-8"?>
<plugin xmlns="http://www.phonegap.com/ns/plugins/1.0"
    xmlns:android="http://schemas.android.com/apk/res/android"
    id="com.phonegap.plugins.localnotification"
    version="3.0.4">

    <name>Child Browser</name>

    <asset src="www/cordova.LocalNotification.js" target="cordova.LocalNotification.js" />    



    <!-- ios -->
    <platform name="ios">
        <plugins-plist key="LocalNotification"
                    string="LocalNotification" />
        <header-file src="src/LocalNotification.h" target="LocalNotification.h"/>        
        <source-file src="src/LocalNotification.m" target="LocalNotification.m"/>        
    </platform>
</plugin>
